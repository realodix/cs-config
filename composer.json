{
    "name": "realodix/cs-config",
    "description": "Shared php-cs-fixer rules & finders preset",
    "license": "MIT",
    "keywords": ["code style","codestyle","coding standards","coding style","php-cs-fixer ","php-cs-fixer-config"],
    "authors": [{"name": "Budi Hermawan", "role": "Developer"}],
    "homepage": "https://github.com/realodix/cs-config",
    "require": {
        "php": "^7.4|^8.0",
        "friendsofphp/php-cs-fixer": "^3.9",
        "kubawerlos/php-cs-fixer-custom-fixers": "^3.11"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Realodix\\CsConfig\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Realodix\\CsConfig\\Tests\\": "tests"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.0.x-dev"
        }
    }
}
